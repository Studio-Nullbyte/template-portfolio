{
  "name": "portfolio-template",
  "version": "1.0.0",
  "description": "A modern, responsive portfolio website template built with Next.js, TypeScript, and Tailwind CSS",
  "author": "Studio Nullbyte",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/Studio-Nullbyte/template-portfolio#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Studio-Nullbyte/template-portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/Studio-Nullbyte/template-portfolio/issues"
  },
  "keywords": [
    "portfolio",
    "nextjs",
    "typescript",
    "tailwindcss",
    "template",
    "responsive",
    "dark-mode",
    "framer-motion",
    "accessibility",
    "seo"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,json,css,md}\"",
    "clean": "rimraf .next out",
    "analyze": "ANALYZE=true npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tooltip": "^1.2.7",
    "axe-core": "^4.10.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-themes": "^0.4.6",
    "pa11y": "^9.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "https://registry.npmjs.org/autoprefixer/-/autoprefixer-10.4.21.tgz",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "typescript": "^5"
  }
}
